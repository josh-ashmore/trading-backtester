  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Pytest Current",
      "type": "debugpy",
      "request": "launch",
      "args": ["${file}"],
      "env": { "PYTHONPATH": "${PYTHONPATH}:${workspaceFolder}" },
      "python": "/Users/joshashmore/Library/Caches/pypoetry/virtualenvs/trading-backtester-qXpROkCl-py3.12/bin/python",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "module": "pytest",
      "justMyCode": false
    },
    {
      "name": "Pytest All",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "justMyCode": false
    },
    {
      "args": ["--nf", "--lf"],
      "name": "Pytest New and Failing",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "justMyCode": false
    }
  ]
}
